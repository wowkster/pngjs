{
  "name": "@wowkster/pngjs",
  "version": "6.0.1",
  "description": "PNG encoder/decoder in pure JS, supporting any bit size & interlace, async & sync with full test suite.",
  "contributors": [
    "Alexandre ParÃ©",
    "Gaurav Mali",
    "Gusts Kaksis",
    "Kuba Niegowski",
    "Luke Page",
    "Pietajan De Potter",
